{
  "name": "uoko-fe-themes-mobile",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode development",
    "build:test": "vue-cli-service build --mode test",
    "build": "vue-cli-service build --mode production && node deploy-to-qn.js",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@types/echarts": "^4.1.1",
    "@types/fastclick": "^1.0.28",
    "@xunlei/vue-lazy-component": "^1.1.3",
    "axios": "^0.18.0",
    "better-scroll": "^1.13.2",
    "echarts": "^4.2.0-rc.2",
    "fastclick": "^1.0.6",
    "highlight.js": "^9.12.0",
    "intersection-observer": "^0.5.1",
    "register-service-worker": "^1.0.0",
    "vant": "^1.3.8",
    "vue": "^2.5.17",
    "vue-axios": "^2.1.4",
    "vue-class-component": "^6.3.2",
    "vue-property-decorator": "^7.2.0",
    "vue-router": "^3.0.1",
    "vue-skeleton-webpack-plugin": "^1.2.2",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.1",
    "vuex-persist": "^1.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.47",
    "@types/axios": "^0.14.0",
    "@types/jest": "^23.1.4",
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-e2e-nightwatch": "^3.0.5",
    "@vue/cli-plugin-pwa": "^3.0.5",
    "@vue/cli-plugin-typescript": "^3.0.5",
    "@vue/cli-plugin-unit-jest": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "@vue/test-utils": "^1.0.0-beta.20",
    "ajv": "^6.5.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.0.1",
    "babel-loader": "^8.0.2",
    "babel-plugin-import": "^1.8.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "jest-css-modules": "^1.1.0",
    "postcss-theme-variables": "^1.0.0",
    "qiniu": "^7.2.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-import-plugin": "^1.5.5",
    "ts-jest": "^23.0.0",
    "typescript": "^3.0.0",
    "vue-jest": "^3.0.1",
    "vue-lazyload": "^1.2.6",
    "vue-template-compiler": "^2.5.17"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "vue-jest",
      "^.+\\.(css)$": "jest-css-modules",
      "^.+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 50
      }
    },
    "globals": {
      "ts-jest": {
        "babelConfig": true
      }
    }
  }
}
